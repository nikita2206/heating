{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdbtarget",
      "request": "attach",
      "name": "Eclipse CDT GDB Adapter"
    },
    {
      "type": "espidf",
      "name": "Launch",
      "request": "launch"
    },
    {
      "name": "Debug single CPP file",
      "program": "${workspaceFolder}/test",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": true,
      "type": "cppdbg",
      "request": "launch",
      "MIMode": "lldb",
      "MIDebuggerPath": "/usr/bin/lldb"
    }
    
  ]
}